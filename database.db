CREATE DATABASE IMarketing;

CREATE TABLE Images(
    id_images INT IDENTITY(1,1) PRIMARY KEY,
    url_1 VARCHAR(100),
    url_2 VARCHAR(100),
    url_3 VARCHAR(100),
    url_4 VARCHAR(100),
    url_5 VARCHAR(100),
);

CREATE TABLE Categories(
    id_category INT IDENTITY(1,1) PRIMARY KEY,
    [name] VARCHAR(100)
    );

CREATE TABLE Products(
    id_product INT IDENTITY(1,1) PRIMARY KEY,
    name VARCHAR(100),
    brand VARCHAR(30),
    descripcion VARCHAR(200),
    is_new BIT,
    quantity INT,
    price DECIMAL(10, 2),
    id_images INT,
    id_category INT,
    FOREIGN KEY(id_images) REFERENCES Images(id_images),
    FOREIGN KEY(id_category) REFERENCES Categories(id_category)
);

CREATE TABLE Sellers(
    id_seller INT IDENTITY(1,1) PRIMARY KEY,
    name VARCHAR(50),
    phone VARCHAR(20),
    email VARCHAR(50),
    address VARCHAR(100),
    city VARCHAR(50),
    department VARCHAR(50),
    country VARCHAR (50),
    is_good_assessment BIT,
);

CREATE TABLE Sales(
    id_sale INT IDENTITY(1,1) PRIMARY KEY,
    id_seller INT,
    id_product INT,
    quantity_sold INT,
    sale_amount DECIMAL(15, 2),
    sale_date DATE,
    FOREIGN KEY(id_seller) REFERENCES Sellers(id_seller),
    FOREIGN KEY(id_product) REFERENCES Products(id_product)
);

